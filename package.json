{
  "name": "nextjs-fullstack-template",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "pnpx tsx prisma/seed/seed.ts"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "test": "./scripts/run-integration-tests.sh",
    "test:unit": "vitest run --coverage --config=vitest.unit-config.ts",
    "test:dev": "vitest",
    "format": "prettier --write .",
    "cm": "cz",
    "migrate": "prisma migrate dev",
    "postmigrate": "npx @snaplet/seed sync",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky"
  },
  "dependencies": {
    "@prisma/client": "^5.15.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@tanstack/react-query": "^5.40.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "next": "14.2.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.5.0",
    "@snaplet/copycat": "^5.0.0",
    "@snaplet/seed": "0.97.20",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^1.6.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^9.0.11",
    "jsdom": "^24.1.0",
    "lint-staged": "^15.2.5",
    "next-test-api-route-handler": "^4.0.8",
    "postcss": "^8.4.38",
    "prettier": "^3.3.1",
    "prettier-plugin-tailwindcss": "^0.6.2",
    "prisma": "^5.15.0",
    "tailwindcss": "^3.4.4",
    "tsx": "^4.16.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "@snaplet/seed": {
    "config": "prisma/seed/seed.config.ts"
  }
}
